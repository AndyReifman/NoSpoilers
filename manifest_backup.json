{
"manifest_version": 2,
    "name": "No Spoilers",
    "description": "Replaces youtube titles to prevent spoilers.",
    "version": "1.0",
    "permissions": ["activeTab"],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}
